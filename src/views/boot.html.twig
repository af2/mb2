{% block navbar %}
    {% include 'includes/navbar.html.twig' %}
{% endblock %}
<header class="container">{% block header %}{% endblock %}</header>
<div style="background:{{ background }};">

  <div class="container">
{% block container %}
      <div id="heroCarousel" class="carousel">
        <!-- Carousel items -->
        <div class="carousel-inner">
          {% for slide in carousel %}
            <div class="{% if loop.first %}active{% endif %} item">
              <img src="{{ slide.image }}" />
              {% if slide.caption is defined %}
                <div class="carousel-caption">
                  <h4>{{ slide.caption.heading }}</h4>
                  <p>{{ slide.caption.body }}</p>
                </div>
              {% endif %}

            </div>
          {% endfor %}
        <!--
          <div class="active item"><img src="img/whiskey1.jpg" /></div>
          <div class="item"><img src="img/whiskey2.jpg" /></div>
          <div class="item"><img src="http://flickholdr.com/900/289/whiskey" /></div-->
        </div>
        <!-- Carousel nav -->
        <a class="carousel-control left" href="#heroCarousel" data-slide="prev">&lsaquo;</a>
        <a class="carousel-control right" href="#heroCarousel" data-slide="next">&rsaquo;</a>
      </div>

      <!-- Main hero unit for a primary marketing message or call to action -->
      <div class="hero-unit">
        <h1>{{ headline }}</h1>
        <p>Welcome to the {{ name }} website. It includes a large callout called the hero unit and three supporting pieces of content. Use it as a starting point to create something more unique.  </p>
        <p><a class="btn btn-primary btn-large">Learn more &raquo;</a></p>
      </div>

{% endblock %}
  </div>

</div>
