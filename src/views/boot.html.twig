	<style>
	body {
	  padding-top: 60px;
	  padding-bottom: 40px;
	}
	</style>
  {% if google_webfonts %}
    {% for font in google_webfonts %}
      <link href='http://fonts.googleapis.com/css?family={{ font }}' rel='stylesheet' type='text/css'>
